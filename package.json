  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "remark-gfm": "^4.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
