    ],
    "nodes": [
      "dist/nodes/MarkdownToNotion/MarkdownToNotion.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
